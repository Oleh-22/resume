<main class="p-5">
      {{#with pageList}}
      <h2>{{title}}</h2>
      <p class="mt-4">{{description}}</p>
      <div>{{text}}</div>
      {{#each button}}
      <div class="mt-3">
               <a class="btn btn-primary" href="{{link}}">{{text}}</a>
      </div>
      {{/each}}
      {{/with}}
      
</main>