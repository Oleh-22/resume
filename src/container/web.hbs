{{!-- 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

{{#with web}}
<section>
   {{> title data="Languages" big=true}}
  <ul>
    <div>
      {{#each languages}}
       
        <hr class="mt-3">
       
        <strong>{{name}}</strong>
        
        <div class="mt-3">{{version}}</div>
        
        <li class="mt-3"><strong>Description</strong></li>
       
        <div class="mt-5 mb-2">{{description}}</div>
        
          {{#each elements}}
           
            <h5 class="mt-3">Elements</h5>
          
            <li class="mt-3">{{name}}</li>
            <br>
            <div class="mb-3">{{description}}</div>
            
            {{#each attributes}}
              <h5>Attributes</h5>
              <p>{{name}}</p>
              <div class="mt-3 mb-3">{{description}}</div>
             
            {{/each}}
          {{/each}}
                  {{#each properties}}
                
                  <h5 class="mt-3 mb-5">Properties</h5>
                 
                  <li class="mt-3 mb-3">{{name}}</li>
                 
                  <div class="mb-3">{{description}}</div>
                  
                    {{#each values}}
                  
                    <h4 class="mt-3">{{value}}</h4>
                   
                    <div class="mt-5">{{description}}</div>
                    {{/each}}
                    {{/each}}
                      {{#each functions}}
                         
                          <h5 class="mt-5">Function</h5>
                         
                            <li class="mt-5">{{name}}</li>
                            <div class="mt-3 mb-3">{{description}}</div>
                         
                        {{#each parameters}}
                       
                        <h5 class="mt-3 mb-3">Parameters</h5>
                        
                       
                          <li  class="mt-3 mb-3">{{name}}</li>
                         
                          <div>{{type}}</div>
                        
                        <div  class="mt-3 mb-3">{{description}}</div>
                        
                        {{/each}}
          {{/each}}
      {{/each}}
     </div>
  </ul>
</section>
{{/with}}
    


