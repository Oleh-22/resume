<main class="p-2">
      <div class="row g-5">
          <div class="col-12 ms-5 p-4">
               <div class="card">
                     <div class="nav nav-tabs">       
                         {{#with navigation}}
                              {{#each links}}
                                   <div class="nav item">
                                        <a href="{{href}}" class="nav-link">{{text}}</a>
                                   </div>
                              {{/each}} 
                                   <div class="nav-item">
                                        <a href="#" class="nav-link disabled">Disabled</a>
                                   </div> 
                        {{/with}} 
                     </div>
               </div> 
          </div>
    <section class="ms-4">
    {{#with header}}
            <h2 class=" ps-5">{{title}}</h2>
            <div class="col-4 ps-5 mt-5">
            {{#with button}}
                     <a class="btn btn-outline-secondary" href="{{link}}">{{text}}</a>
            {{/with}}
            </div>
    {{/with}}

    <hr class="col-12 ms-5 mt-5 ">
    </section>

     <section class="ms-4">
             {{#each goodsBlock}}
                    <div class="card col-12 ms-5 "> 
                                       
                                   <div class="card-body">
                                         <div class="row ">
                                        <div class="col-2">
                                         <img class="card-img-top" src="{{image}}" alt="Product photo">
                                         </div>
                                         <div class="col-4">
                                       <h2>{{name}}</h2>
                                       <p>{{description}}</p>
                                        </div>
                                        <div class="col-2 mt-5 ms-5">
                                             {{#with price}}
                                                     <h5>{{title}}:</h5>
                                                     <h3>{{value}}</h3>
                                             {{/with}}
                                        </div>
                                         <div class="col-1 mt-3">
                                               <h5 class="mt-5">{{amount}}</h5>
                                         </div>
                                           <div class="col-2 ms-3 mt-2 ">
                                                {{#each buttons}}
                                                       {{#if isPrimary}}
                                                       <button class="btn btn-primary mt-3">{{text}}</button>
                                                       {{else}}
                                                       <button class="btn btn-outline-danger mt-3">{{text}}</button>
                                                       {{/if}}
                                                {{/each}}
                                           </div>
                                   </div>
                                   </div>
                    </div>
             {{/each}}

              <hr class="col-12 ms-5 mt-5 pt-4 ">
     </section>
     <section class="ms-4">
             {{#with total}}
                     <h2 class="ms-5">{{title}}</h2>
                     <h1 class="ms-5 mt-4">{{amount}}</h1>
                     <h5 class="ms-5 mt-4">{{delivery}}</h5>
                     <div class="mt-4">
                          {{#each buttons}}
                                        {{#if isSuccess}}
                                              <a class="btn btn-success ms-5" href="{{link}}">{{text}}</a>
                                         {{/if}}
                                         <div class="mt-4">
                                          {{#if isOutline}}
                                              <a class="btn btn-outline-success ms-5" href="{{link}}">{{text}}</a>
                                         {{/if}}
                                         </div>
                          {{/each}}
                     </div>
             {{/with}}

              <hr class="col-12 ms-5 mt-5 pt-4 ">
     </section>
     <section class="ms-4">
              {{#with goodsOtherBlock}}
                       <h2 class="text-center">{{title}}</h2>
                        <div class="row g-2 ms-5 ">
                       {{#each cards}}
                               <div class="card col-4 "> 
                                        <div class="row g-3">
                                   <div class="card-body"> 
                                   <table class="table table-bordered ">
                                             <img class="card-img-top" src="{{image}}" alt="Product photo">
                                             <div class="card-title  p-3">{{title}}
                                                  {{#if isHot}}
                                                            <span class="badge text-bg-warning rounded-pill text-uppercase">HOT!</span>
                                                  {{/if}}
                                                  {{#if isNew}}
                                                            <span class="badge text-bg-info rounded-pill text-uppercase">New</span>
                                                  {{/if}}
                                             </div>
                                             <p class="p-2">{{description}}</p>
                                   </table>
                                   </div>
                                   </div>
                                   </div>
                       {{/each}}
              {{/with}}

               <hr class="col-12 ms-2 mt-5 pt-3 ">
     </section>
      <div class="col-12 ms-5 mt-5">
               {{#with subscribe}}
                       <div class="card col-12 ms-3">
                       <div class="card-body text-bg-dark">
                             <h2>{{header}}</h2>
                             <p class="mt-3">{{description}}</p>
                              {{#each buttons}}
                              <div class="row">
                                   <div class="col-3 p-2">
                                   <a class="btn btn-primary" href="{{link}}">{{text}}</a>
                                   </div>
                              </div>
                              {{/each}} 
                       </div>
                       </div>  
               {{/with}}
            </div>

             <hr class="col-12 ms-5 mt-5 ">
     
      <div>     
                 <div class="nav ms-5">
                     {{#each footer}}
                         <div class="nav-item">
                              {{#each this}}  
                                   {{#if @last}}
                                        <a href="{{link}}" class="nav-link disabled">{{text}}</a>
                                   {{else}}
                                        <a href="{{link}}" class="nav-link active">{{text}}</a>
                                   {{/if}}
                              {{/each}}
                         </div>
                     {{/each}}
       </div>
       
       
</main>