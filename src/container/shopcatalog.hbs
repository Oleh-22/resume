<main class="p-2">
      <div class="row g-5">
          <div class="col-12 ms-4 p-4">
               <div class="card">
                     <div class="nav nav-tabs">       
                         {{#with navigation}}
                              {{#each links}}
                                   <div class="nav item">
                                        <a href="{{href}}" class="nav-link">{{text}}</a>
                                   </div>
                              {{/each}} 
                                   <div class="nav-item">
                                        <a href="#" class="nav-link disabled">Disabled</a>
                                   </div> 
                        {{/with}} 
                     </div>
               </div> 
          </div>
    <section class="ms-4">
   
                      <div class="row">
                  {{#each breadcrumb}}
              <div class="col-12">
                   
                        <div class="breadcrumb">          
                             {{#if @last}}
                             <div class="breadcrumb-item active">{{name}}</div>
                             {{else}}
                              <div class="breadcrumb-item">
                                    <a href="{{url}}">{{name}}</a>
                              </div>
                              {{/if}}
                         </div>
                   </div>
                    {{/each}}
                       
               
     </section>     
     <section class="ms-4">
         <h1>{{header}}</h1>   
         <div class="row">
         {{#each sortBlock}} 
         <div class="col-3 mt-4">
          <select class="form-select">
                   <option selected>{{placeholder}}</option>
                     {{#each options}}
                         <option value="{{value}}">{{text}}</option>
                     {{/each}}
          </select>
          </div>
         {{/each}}
     </section>  
         
     <section class="ms-4">
                <div class="row">
                    <div class="col-4">
                         {{#with filterBlock}}
                                   <div class="card ">
                                         <div class="card-body">
                                 {{#with sellers}}
                                         <p>{{title}}</p>
                                         <div class="row"></div>
                                         {{#each list}}
                                         <div class="col-5 mt-3">
                                                 <input class="form-check-input" type="checkbox">
                                        <label class="form-check-label" for="form-check-input">
                                        {{value}}
                                        </label>  
                                        </div>
                                         {{/each}}
                                 {{/with}}
                                 <hr>
                                 {{#with brands}}
                                 <p>{{title}}</p>
                                 {{#with input}}
                                          <input class="form-control" placeholder="{{placeholder}}">
                                 {{/with}}
                                 <div class="row "></div>                                
                                   {{#each options}}
                                         <div class="col-5 mt-3">
                                                 <input class="form-check-input" type="checkbox">
                                        <label class="form-check-label" for="form-check-input">
                                        {{value}}
                                        </label>  
                                      </div>
                                 {{/each}}
                                 {{/with}}
                                 <hr>
                                 {{#with price}}
                                          <p>{{title}}</p>
                                          {{#with range}}
                                                       <input class="form-control" placeholder="{{from}}">
                                                        <input class="form-control mt-3" placeholder="{{to}}">
                                        {{value}}
                                        </label> 
                                          {{/with}}
                                          {{#with button}}
                                                    <a class="btn btn-secondary mt-3" href="{{link}}">{{text}}</a>
                                          {{/with}}
                                 {{/with}}
                                 <hr>
                                 {{#each filterList}}
                                         <p>{{title}}</p>
                                         <div class="row "></div>
                                           
                                         {{#each options}}
                                         
                                          <div class="col-8 mt-3">
                                                            <input class="form-check-input" type="checkbox">
                                        <label class="form-check-label" for="form-check-input">
                                        {{value}}
                                        </label>  
                                        </div>
                                         {{/each}}
                                         <hr>
                                 {{/each}}
                                         </div>
                                    </div>
                         {{/with}}
                    </div>
                    
                     <div class="col-8">
                      
                       
                        <div class="row">
                          {{#each goodsList}}
                           <div class="col-6">    
                                   <div class="card mt-5">  
                                         <div class="card-body">
                                             
                                               <img class="card-img-top " src="{{img}}" alt="Product photo">
                                               {{#if isHot}}
                                                  <span class="badge text-bg-warning rounded-pill text-uppercase">HOT</span>
                                             {{/if}}
                                             {{#if isNew}}
                                                  <span class="badge text-bg-success rounded-pill text-uppercase">New</span>
                                             {{/if}}
                                             <br>
                                              {{#if isFast}}
                                                  <a class="btn btn-success btn-sm mt-3" href="#">Швидка доставка</a>
                                             {{/if}}
                                              <br>
                                              {{#if isFree}}
                                                  <a class="btn btn-secondary btn-sm mt-3" href="#">Безкоштовна доставка</a>
                                             {{/if}}
                                             <h5 class="mt-3">{{name}}</h5>
                                             {{#with review}}
                                                    <p>{{amount}} {{text}}</p>
                                             {{/with}}
                                             <h4 class="mt-4">{{price}}</h4>
                                             <p>{{delivery}}</p>
                                             {{#each buttons}}
                                                      {{#if isPrimary}}
                                                      <a class="btn btn-primary mt-3" href="{{link}}">{{text}}</a>
                                                      {{/if}}
                                                       {{#if isSecondary}}
                                                      <a class="btn btn-secondary mt-3" href="{{link}}">{{text}}</a>
                                                      {{/if}}
                                             {{/each}}
                                             {{#each characteristics}}
                                                    <ul class="mt-3">
                                                       <li>{{title}}: {{value}}</li>
                                                    </ul>
                                             {{/each}}
                                        </div>
                                   </div>
                              </div>
                          {{/each}}           
                     </div>
     </section>

       {{#with service}}
                       <div class="card ms-3">
                         <div class="card-body text-bg-success text-center">
                              <h2>{{title}}</h2>
                              <p class="mt-3">{{description}}</p>
                                   {{#each buttons}}
                                   <div class="row">
                                        <div class="text-center">
                                             <a class="btn btn-primary " href="{{link}}">{{text}}</a>
                                        </div>
                                   </div>
                                   {{/each}}
                         </div>
                       </div>
               {{/with}}
          



 <hr class="mt-5">

          <div>     
                 <div class="nav">
                     {{#each footer}}
                         <div class="nav-item">
                              {{#each this}}  
                                   {{#if @last}}
                                        <a href="{{link}}" class="nav-link disabled">{{text}}</a>
                                   {{else}}
                                        <a href="{{link}}" class="nav-link active">{{text}}</a>
                                   {{/if}}
                              {{/each}}
                         </div>
                     {{/each}}
          </div>

       
</main>
 